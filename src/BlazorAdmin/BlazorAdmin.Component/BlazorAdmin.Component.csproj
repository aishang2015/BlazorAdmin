  <ItemGroup>
    <ProjectReference Include="..\BlazorAdmin.Servers.Core\BlazorAdmin.Servers.Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>
  <ItemGroup>
